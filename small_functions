    print()


def partf():
    print()


#testing the parts down below
print(parta(9,2))
print(partb(3))
print(partc('*', 'Dr. Ritchey', 'Texas A&M University','snritchey@tamu.edu'))
a = [1,2,4,5,7,6,9]
